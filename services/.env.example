# PhotoIdentifier Services Environment Variables

# Application
APP_NAME=PhotoIdentifier Services
APP_VERSION=0.1.0
DEBUG=True

# Database
DATABASE_URL=postgresql://photoidentifier:photoidentifier_dev_password@localhost:5432/photoidentifier

# Redis
REDIS_URL=redis://localhost:6379/0

# Security
SECRET_KEY=your-secret-key-change-in-production
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# File Upload
MAX_UPLOAD_SIZE=10485760
UPLOAD_DIR=uploads

# Elasticsearch
ELASTICSEARCH_URL=http://localhost:9200
ELASTICSEARCH_INDEX_PREFIX=photoidentifier

# External Services
OPENAI_API_KEY=
ANTHROPIC_API_KEY=

# Notification Services
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=
SMTP_PASSWORD=
EMAIL_FROM=noreply@photoidentifier.com

# Marketplace
STRIPE_API_KEY=
STRIPE_WEBHOOK_SECRET=

# Telehealth
TELEHEALTH_ENABLED=False
BOOKING_MAX_DAYS_IN_ADVANCE=30

# CORS
CORS_ORIGINS=["http://localhost:3000","http://localhost:3001","http://127.0.0.1:3000"]
